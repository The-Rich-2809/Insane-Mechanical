@model IEnumerable<Insane_Mechanical.Models.Articulo>
@{
    if (ViewBag.Nivel == "Cliente")
    {
        Layout = "_LayoutCliente";
    }
    if (ViewBag.Nivel == "Admin")
    {
        Layout = "_LayoutAdmin";
    }
}
<!DOCTYPE html>
<html>
<head>
    <title>Vista Principal</title>
    <link rel="canonical" href="https://getbootstrap.com/docs/5.1/examples/album/">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>
</head>
<body>
    @{
        foreach(var i in Model)
        {
            if(i.RutaHTML == ViewBag.RutaHTML)
            {
                @Html.Partial(i.RutaHTML)
                @Html.Partial("../Shared/Comentarios.cshtml");
                break;
            }
        }
    }
    <!-- O usando RenderPartial -->
</body>
</html> 